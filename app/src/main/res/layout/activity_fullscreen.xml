<?xml version="1.0" encoding="utf-8"?>

<FrameLayout tools:context=".FullscreenActivity"
    android:background="#0099cc"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- The primary full-screen view. This can be replaced with whatever viewis needed to present your content, e.g. VideoView, SurfaceView,TextureView, etc. -->


    <!-- This FrameLayout insets its children based on system windows usingandroid:fitsSystemWindows. -->


    <FrameLayout android:background="#FFFFFF" android:layout_height="match_parent" android:layout_width="match_parent" android:fitsSystemWindows="true">

        <Button
            android:id="@+id/button10"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="220dp"
            android:text="7" />

        <Button
            android:id="@+id/button11"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="220dp"
            android:text="8" />

        <Button
            android:id="@+id/button12"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="220dp"
            android:text="9" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="300dp"
            android:text="4" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="300dp"
            android:text="5" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="300dp"
            android:text="6" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="380dp"
            android:text="1" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="380dp"
            android:text="2" />

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="380dp"
            android:text="3" />

        <Button
            android:id="@+id/buttondiv"
            android:layout_width="52dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="150dp"
            android:text="/" />

        <Button
            android:id="@+id/buttoncanc"
            android:layout_width="52dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="150dp"
            android:text="C" />

        <Button
            android:id="@+id/buttonLog"
            android:layout_width="52dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="560dp"
            android:contentDescription="Log("
            android:text="Log" />

        <Button
            android:id="@+id/buttonmul"
            android:layout_width="52dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="150dp"
            android:text="X" />

        <Button
            android:id="@+id/buttonsub"
            android:layout_width="52dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="150dp"
            android:text="-" />

        <Button
            android:id="@+id/buttoneql"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:layout_marginTop="80dp"
            android:text="=" />

        <Button
            android:id="@+id/buttonadd"
            android:layout_width="52dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="150dp"
            android:text="+" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="30sp" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="460dp"
            android:text="." />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="460dp"
            android:text="0" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="78dp"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="460dp"
            android:text="+/-" />



    </FrameLayout>
</FrameLayout>

